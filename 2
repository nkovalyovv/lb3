#include <windows.h>
#include <string>
#include <iostream>
#include <cctype>

using namespace std;

int main() {
    setlocale(LC_ALL, "ru_RU");
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    string str;
    cout << "Введите строку: ";
    getline(cin, str);
    str = "йцукенгшщзхъфывапролдячсмить";

    int lc = 0;
    int dc = 0;

    for (char ch : str) {
        if (isdigit(ch)) {
            dc++;
        }
        else if (isalpha(ch) || str.find(ch) != -1) {
            lc++;
        }
    }

    if (dc > lc) {
        cout << "В строке больше цифр" << endl;
    }
    else if (dc < lc) {
        cout << "В строке больше букв" << endl;
    }
    else {
        cout << "Количество цифр и букв в строке равно" << endl;
    }

    return 0;

}
